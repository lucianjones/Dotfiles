{
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "for-direction": 2,
        "getter-return": 2,
        "no-async-promise-executor": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-else-if": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-global-assign": 2,
        "no-import-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-misleading-character-class": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-symbol": 2,
        "no-octal": 2,
        "no-prototype-builtins": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-setter-return": 2,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-useless-catch": 2,
        "no-useless-escape": 2,
        "no-with": 2,
        "require-yield": 2,
        "valid-typeof": 2,
        "array-bracket-spacing": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": 2,
        "brace-style": 2,
        "camelcase": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "curly": 2,
        "implicit-arrow-linebreak": 2,
        "indent": 2,
        "jsx-quotes": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "line-comment-position": 2,
        "linebreak-style": 2,
        "no-confusing-arrow": 2,
        "no-duplicate-imports": 2,
        "no-extra-parens": 2,
        "no-undef": 2,
        "no-trailing-spaces": 2,
        "no-unused-expressions": 2,
        "no-useless-return": 2,
        "no-var": 2,
        "object-curly-newline": 2,
        "object-curly-spacing": [2,"always"],
        "prefer-const": 2,
        "prefer-destructuring": 2,
        "quotes": [2, "single",{ "allowTemplateLiterals": true }],
        "semi-spacing": 2,
        "semi-style": 2,
        "sort-imports": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": 2,
        "space-in-parens": 2,
        "spaced-comment": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "jest": true,
        "es6": true,
        "es2017": true,
        "es2020": true,
        "es2021": true
    }
}
